  const baseUrl = 'https://www.dmv.ca.gov/wasapp/ipp2/checkPers.do';

  for (const word of words) {
    const plate = word.toUpperCase().slice(0, 7);

    // Must be at least 2 characters to avoid "two.characters" validation error
    if (plate.length < 2) {
      console.warn(`âš ï¸ Skipping "${plate}" â€” too short.`);
      continue;
    }

    const formData = new URLSearchParams({
      plateType: 'Z',
      kidsPlate: '',
      plateNameLow: 'california 1960s legacy',
      plateName: 'California 1960s Legacy',
      plateLength: '7',
      vetDecalCd: '',
      centeredPlateLength: '0',
      platechecked: 'no',
      imageSelected: 'none',
      vehicleType: 'AUTO',
      vetDecalDesc: ''
    });

    // Dynamically fill plateChar fields with actual letters, then blanks
    for (let i = 0; i < 14; i++) {
      formData.append(`plateChar${i}`, plate[i] || '');
    }

    try {
      const res = await fetch(baseUrl, {
        method: 'POST',
        headers: {
          'Content-Type': 'application/x-www-form-urlencoded'
        },
        body: formData.toString()
      });

      const json = await res.json();

      if (json.success && json.code === 'AVAILABLE') {
        console.log(`âœ… ${plate} is Available`);
      } else if (json.code === 'VALIDATION') {
        console.log(`âš ï¸ ${plate} is Invalid (${json.message})`);
      } else {
        //console.log(`âŒ ${plate} is Taken`);
      }
    } catch (err) {
      console.error(`ðŸš« Error checking "${plate}":`, err);
    }

    //await new Promise(r => setTimeout(r, 100)); // Delay to prevent rate limit
  }
})();
